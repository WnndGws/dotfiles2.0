set autocalc=1
set numeric=0
set xlsx_readformulas=1
set external_functions=1
set half_page_scroll=1
set ignorecase=1

nmap "ZZ" ":x!<cr>"
nmap ",w" ":w<cr>"
nmap "ZQ" ":q!<cr>"

nmap "pc" "Pc"

color "type=HEADINGS fg=WHITE bg=BLUE"
color "type=CELL_SELECTION fg=WHITE bg=MAGENTA"
color "type=CELL_SELECTION_SC fg=WHITE bg=CYAN"
