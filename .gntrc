[colors]
# The RGB values range in [0, 1000] 
black = 46,52,54
red = 204,0,0
green = 78,154,6
blue = 52,101,164
white = 211,215,207
gray = 85,87,83
darkgray = 2,26,32

[colorpairs]
normal = default; default
highlight = black; gray
shadow = black; darkgray
title = white; blue
titled = white; gray
text = white; blue
disabled = gray; black
urgent = green; black

[Finch]
wm = /usr/lib/gnt/irssi.so
color-available = green; darkgray
color-away = blue; darkgray
color-idle = gray; darkgray
color-offline = gray; darkgray
color-message-sent = yellow; default
color-message-received = yellow; default
color-message-highlight = red; default
color-message-action = green; yellow
color-timestamp = blue; default

[GntWM::binding]
c-l = window-next
c-h = window-prev
c-x = window-close
c-w = window-list
c-d = dump-screen
c-, = shift-left
c-. = shift-right
c-a = action-list
c-m = start-move
c-r = start-resize
c-q = window-close
